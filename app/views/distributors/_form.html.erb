<%= form_for(distributor) do |f| %>
  <% if distributor.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(distributor.errors.count, "error") %> prohibited this distributor from being saved:</h2>

      <ul>
      <% distributor.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group">
  <% f.label :name %>
  <%= f.text_field :name, placeholder: "Distributor Name",class: "form-control" %>
  </div>
  <div class="form-group">
   <% f.label :email %>
   <%= f.email_field :email, placeholder: "Distributor Email",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :phone %>
   <%= f.text_field :phone, placeholder: "Distributor Phone",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :flat_houseno %>
   <%= f.text_field :flat_houseno, placeholder: "Distributor Shop number",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :street %>
   <%= f.text_field :street, placeholder: "Distributor Shop street",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :area %>
   <%= f.text_field :area, placeholder: "Distributor area",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :city %>
   <%= f.text_field :city, placeholder: "Distributor City",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :State %>
   <%= f.text_field :state, placeholder: "Distributor State",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :Country %>
   <%= f.text_field :country, placeholder: "Distributor Country",class: "form-control" %>
 </div>
 <div class="form-group">
   <% f.label :Pin %>
   <%= f.text_field :pin, placeholder: "Distributor Pincode",class: "form-control" %>
